SELECT gum.*, 
u2.formatted_name 

FROM greenhouse_usermapping gum 

INNER JOIN simplrcore_user u ON u.organization_id = 69 
INNER JOIN simplrcore_user u2 ON u2.id = gum.simppler_user_id 

WHERE gum.greenhouse_stage IS NOT NULL 

GROUP BY gum.simppler_user_id, gum.greenhouse_stage 
ORDER BY gum.greenhouse_stage;
