SELECT 
o.first_name, 
o.last_name, 
o.email, 
ut.value as user_type, 
o.invitation_attempts as "invitations",
saw_onboarding_page as "opened email", 
account_created as "account created"/*, 
count(e.from_user_id) as connections*/, 
last_invitation_sent_on_date as created_date, 
o.updated_at AS signed_up_date 

FROM onboarding_user o

JOIN simplrcore_user u ON u.email=o.email
JOIN onboarding_userchecklist uc ON uc.user_id=u.id
JOIN simplrcore_usertype ut ON ut.id=u.type_id

WHERE o.organization_id=109
