SELECT 
adv.eid as advertisable_eid, c.eid as campaign_eid, c.status, c.updated_date

FROM 
advertisables adv
JOIN campaigns c on c.advertisable_id=adv.id
JOIN campaign_budget_allocations cba on cba.campaign_id=c.id
JOIN organizations o on o.id = adv.organization_id

WHERE 

o.ops_organization_id = '8677'										
AND o.sales_organization_id IN(8683,29645,105096,105103,86359,	86361,	86368,	86094,	86367,	86314,	105097,	86362,	105102,	86352,	86350,	105098,	86353,	77254,	77281,	138195,	109469,	138203,	38055,	81797,	97548,	115175,	53179,	77259,	139882,	126863,	146812,	77283,	97533,	81804,	71813,	139955,	71815,	144959,	71819,	71816,	145032,	144955,	97544,	53180,	77261,	97540,	126866,	81811,	120317,	65856,	77282,	53183,	146811,	53176,	53175,	77284,	126850,	138204,	138190,	71809,	109466,	71812,	109462,	81805,	93874,	77250,	109463,	145015,	120333,	65859,	145031,	146337,	97551,	145025,	120316,	120315,	97537,	42495,	109472,	120339,	97491,	53178,	144953,	152472,	97490,	126865,	109459,	152477,	120326,	126857,	81820,	146810,	71799,	97532,	109464,	145033,	144952,	81801,	126856,	138200,	81799,	138207,	152475,	81803,	126867,	126849,	71807,	120313,	97495,	152473,	138198,	152478,	42496,	81800,	109467,	97531,	36374,	139950,	138205,	65860,	144960,	115177,	38056,	97539,	81796,	144956,	37491,	115182,	38058,	43411,	120322,	71806,	71811,	115179,	138191,	71802,	115180,	120323,	138206,	144957,	146807,	65858,	126853,	115174,	77285,	138210,	109474,	126855,	138196,	65854,	115172,	144954,	97536,	152476,	97546,	71804,	81806,	109471,	77251,	115176,	77257,	138199,	53181,	97547,	71805,	145030,	77253,	126859,	71814,	126852,	97543,	39294,	138209,	120324,	71810,	81810,	152474,	138192,	152479,	120335,	126858,	43412,	120319,	115170,	77255,	81807,	120337,	126862,	36377,	109461,	42737,	81814)
group by 1,2,3,4
LIMIT 10;
